{
    "name" : "Bookmark Confirmation",
    "version" : "1.0.0",
    "description" : "This is a very basic extension that gives a confirmation popup when user bookmarked page is succesfully saved!",
    "manifest_version" : 2,

    "background" : {
        "scripts" : ["background.js"],
        "persistent" : false
    },

    "permissions" : [
        "bookmarks"
    ],

    "icons" : {
        "16" : "bookmark.png",
        "48" : "bookmark.png",
        "128" : "bookmark.png"

    },

    "browser_action" : {
        "default_title" : "created by Harshith Reddy",
        "default_popup" : "popup.html"
    }
}